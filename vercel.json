{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram",
      "dest": "/api/telegram.js"
    },
    {
      "src": "/api/update-data",
      "dest": "/api/update-data.js"
    }
  ],
  "crons": [
    {
      "path": "/api/update-data",
      "schedule": "0 0 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
